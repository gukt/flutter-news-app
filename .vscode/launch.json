{
    "configurations": [
        {
            "name": "News-app (debug)",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart"
        },
        {
            "name": "News-app (profile)",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "args": [
                "--profile"
            ]
        },
        {
            "name": "Python: Flask",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "mock-api-server/app.py",
                "FLASK_ENV": "development"
            },
            "args": [
                "run",
                "--no-debugger",
                "--host=0.0.0.0"
            ],
            "jinja": true
        }
    ]
}